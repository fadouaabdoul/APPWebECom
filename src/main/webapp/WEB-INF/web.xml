<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet-mapping>
        <servlet-name>ServletHome</servlet-name>
        <url-pattern>/Home</url-pattern>
    </servlet-mapping>
    <servlet>
    <servlet-name>ServletHome</servlet-name>
    <servlet-class>com.APPWebECom.APPWebECom.Servlet.ServletHome</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ServletClientCreate</servlet-name>
        <url-pattern>/Client</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ServletClientCreate</servlet-name>
        <servlet-class>com.APPWebECom.APPWebECom.Servlet.ServletClientCreate</servlet-class>
    </servlet>

    <servlet-mapping>
        <servlet-name>ServletCommandeCreate</servlet-name>
        <url-pattern>/Commande</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>ServletCommandeCreate</servlet-name>
        <servlet-class>com.APPWebECom.APPWebECom.Servlet.ServletCommandeCreate</servlet-class>
    </servlet>


    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>


